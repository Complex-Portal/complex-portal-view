    "build-prod": "rm -rf dist; node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration production --base-href \"/complexportal/\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
