{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "complex-portal-view": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "./dist",
              "browser": ""
            },
            "index": "src/index.html",
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.json",
            "allowedCommonJsDependencies": ["*"],
            "polyfills": [
              "src/polyfills.ts"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ],
            "styles": [
              "src/styles.scss",
              "src/assets/styles/css/complex-portal-theme.scss",
              "node_modules/complexviewer/src/css/xinet.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/assets/styles/scss/material.scss",
              "node_modules/animate.css/animate.css"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.slim.js",
              "node_modules/ebi-framework/libraries/foundation-6/js/foundation.js",
              "node_modules/ebi-framework/js/foundationExtendEBI.js",
              "node_modules/ebi-framework/js/script.js"
            ],
            "aot": false,
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "test": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test.ts"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "complex-portal-view:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "complex-portal-view:build:production"
            },
            "test": {
              "buildTarget": "complex-portal-view:build:test"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "complex-portal-view:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/jquery/dist/jquery.js",
              "node_modules/ebi-framework/libraries/foundation-6/js/foundation.js",
              "node_modules/ebi-framework/js/foundationExtendEBI.js",
              "node_modules/ebi-framework/js/script.js",
              "node_modules/complexviewer/dist/complexviewer.js"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/ngx-toastr/toastr.css",
              "src/assets/styles/scss/material.scss",
              "node_modules/animate.css/animate.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico"
            ]
          }
        }
      }
    },
    "complex-portal-view-e2e": {
      "root": "e2e",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "complex-portal-view:serve"
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "cp",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "cp"
    }
  },
  "cli": {
    "analytics": "317dc215-2845-4c92-bffe-fd11a529bc54"
  }
}
